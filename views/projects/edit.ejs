<% include ../partials/header %>
<div class="ui container">
  <form class="ui form" action="/projects/<%= project._id %>?_method=PUT" method="post">
    <div class="field">
      <label>Title</label>
      <input type="text" name="project[title]" value="<%= project.title %>">
    </div>
    <div class="field">
      <label>Image</label>
      <input type="text" name="project[image]" value="<%= project.image %>">
    </div>
    <div class="field">
      <label>Description</label>
      <textarea name="project[description]"><%= project.description %></textarea>
    </div>
    <button type="submit" class="ui button">Update</button>
  </form>
</div>





<% include ../partials/footer %>
